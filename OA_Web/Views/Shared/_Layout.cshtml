<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Gas share</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/site/css/main.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="https://kit.fontawesome.com/8d755fdf5b.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <style>
        @@import url('https://fonts.googleapis.com/css2?family=Quicksand:wght@400;500;600;700&display=swap');

        * {
            font-family: 'Quicksand', sans-serif;
        }

        #pageMessages {
            position: fixed;
            top: 90px;
            right: 15px;
            z-index: 8;
            width: 30%;
            transition: all 0.3s;
        }

        

        .alert {
            position: relative;
        }

        .alert .close {
            position: absolute;
            top: 5px;
            right: 5px;
            font-size: 1em;
        }

        .alert .fa {
            margin-right: .3em;
        }

        body {
            padding-top: 76px !important;
            min-height: 100vh;
        }

        main {
            min-height: 49vh !important;
        }

        .icon {
            cursor: pointer;
            margin-left: 25px;
            margin-top: 10px
        }

        .icon span {
            border-radius: 50%;
            color: #fff;
            vertical-align: top;
            margin-right: -6px;
            margin-top: 6px;
        }

        }

        .icon img {
            display: inline-block;
            width: 26px;
            margin-top: 4px
        }

        .icon:hover {
            opacity: .7
        }

        .logo {
            flex: 1;
            margin-left: 50px;
            color: #eee;
            font-size: 20px;
            font-family: monospace
        }

        .notifications {
            width: 300px;
            max-height: 440px;
            overflow: scroll;
            display: none;
            position: fixed;
            bottom: 85px;
            right: 25px;
            border-radius: 20px 20px 0px 20px;
            background-color: #fff;
            z-index: 15;
            box-shadow: 0 4px 8px 0 rgb(0 0 0 / 20%), 0 6px 20px 20px rgb(0 0 0 / 19%);
        }

            .notifications h2 {
                font-size: 14px;
                padding: 10px;
                border-bottom: 1px solid #eee;
                color: #999
            }

        .notifications h2 span {
            color: #f00
        }

        .notifications-item {
            display: flex;
            border-bottom: 1px solid #eee;
            padding: 6px 9px;
            margin-bottom: 0px;
            cursor: pointer
        }

        .notifications-item:hover {
            background-color: #eee
        }

        .notifications-item img {
            display: block;
            width: 50px;
            height: 50px;
            margin-right: 9px;
            border-radius: 50%;
            margin-top: 2px
        }

        .notifications-item .text h6 {
            color: #777;
            font-size: 12px;
            margin-top: 3px
        }

        .notifications-item .text p {
            color: #aaa;
            font-size: 16px
        }

        .shadow {
            box-shadow: 0 10px 10px 0 rgb(0 0 0 / 20%), 0 6px 20px 20px rgb(0 0 0 / 19%) !important;
        }

        .bellBtn {
            left: calc(100% - 80px);
            bottom: 25px;
            cursor: pointer;
            z-index: 15;
            transition: background-color .3s;
        }

        .bellBtn:hover {
            background-color: #333;
        }

        @@media (max-width: 768px) {
            #About {
                display: none;
            }
        }


        @@media (max-width: 575px){
            #About {
                display: block;
            }

            #pageMessages {
                width: 60%;                
            }
        }

        
        html {
            overflow: scroll;
            overflow-x: hidden;
        }

        ::-webkit-scrollbar {
            width: 0; /* Remove scrollbar space */
            background: transparent; /* Optional: just make scrollbar invisible */
        }
        /* Optional: show position indicator in red */
        ::-webkit-scrollbar-thumb {
            background: #FF0000;
        }
    </style>

    @await RenderSectionAsync("Styles", required: false)
</head>
