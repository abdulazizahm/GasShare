    <script>
        function createAlert(title, summary, details, severity, dismissible, autoDismiss, appendToId) {
            var iconMap = {
                info: "fa fa-info-circle",
                success: "fa fa-thumbs-up",
                warning: "fa fa-exclamation-triangle",
                danger: "fa ffa fa-exclamation-circle"
            };

            var iconAdded = false;

            var alertClasses = ["alert", "animated", "flipInX"];
            alertClasses.push("alert-" + severity.toLowerCase());

            if (dismissible) {
                alertClasses.push("alert-dismissible");
            }

            var msgIcon = $("<i />", {
                "class": iconMap[severity] // you need to quote "class" since it's a reserved keyword
            });

            var msg = $("<div />", {
                "class": alertClasses.join(" ") // you need to quote "class" since it's a reserved keyword
            });

            if (title) {
                var msgTitle = $("<h4 />", {
                    html: title
                }).appendTo(msg);

                if (!iconAdded) {
                    msgTitle.prepend(msgIcon);
                    iconAdded = true;
                }
            }

            if (summary) {
                var msgSummary = $("<strong />", {
                    html: summary
                }).appendTo(msg);

                if (!iconAdded) {
                    msgSummary.prepend(msgIcon);
                    iconAdded = true;
                }
            }

            if (details) {
                var msgDetails = $("<p />", {
                    html: details
                }).appendTo(msg);

                if (!iconAdded) {
                    msgDetails.prepend(msgIcon);
                    iconAdded = true;
                }
            }


            if (dismissible) {
                var msgClose = $("<span />", {
                    "class": "close", // you need to quote "class" since it's a reserved keyword
                    "data-dismiss": "alert",
                    html: "<i class='fa fa-times-circle'></i>"
                }).appendTo(msg);
            }

            $('#' + appendToId).prepend(msg);

            if (autoDismiss) {
                setTimeout(function () {
                    msg.addClass("flipOutX");
                    setTimeout(function () {
                        msg.remove();
                    }, 1000);
                }, 5000);
            }
        }


        $(document).ready(function () {

            setInterval(GetNotifications, 10000);

            GetNotifications();
            function GetNotifications() {
                $.get("/Notification/Get", function (data) {
                    $("#box").html(data);
                    $("#notificationCount").html((data.match(/UnseenNotification/g) || []).length)
                });
            }



            var down = false;

            $('#bell').click(function (e) {
                if ($("#notificationCount").html() != "0") {
                    $.get("/Notification/AllSeen", function (data) {
                    });
                }


                var color = $(this).text();
                if (down) {

                    $('#box').css('display', 'none');
                    GetNotifications();
                    down = false;
                } else {

                    $('#box').css('display', 'block');
                    down = true;

                }

            });

            resetJourneyActions();




            btn.on('click', function (e) {
                e.preventDefault();
                $('html, body').animate({ scrollTop: 0 }, '300');
            });


        });


        function resetJourneyActions() {
            $(".accordian").on("click", ".accordian-title", function () {
                $(this).toggleClass("active").next().slideToggle(500);
            });


            var el = document.querySelector('.more');
            var btn = el.querySelector('.more-btn');
            var menu = el.querySelector('.more-menu');
            var visible = false;

            function showMenu(e) {
                e.preventDefault();
                if (!visible) {
                    visible = true;
                    el.classList.add('show-more-menu');
                    menu.setAttribute('aria-hidden', false);
                    document.addEventListener('mousedown', hideMenu, false);
                }
            }

            function hideMenu(e) {
                if (btn.contains(e.target)) {
                    return;
                }
                if (visible) {
                    visible = false;
                    el.classList.remove('show-more-menu');
                    menu.setAttribute('aria-hidden', true);
                    document.removeEventListener('mousedown', hideMenu);
                }
            }

            btn.addEventListener('click', showMenu, false);



        }


        function addComment(e) {
            e.preventDefault();


            var submitBtn = $("input[type=submit]", e.target);
            var commentId = $("input[name=Id]", e.target);

            if (submitBtn.val() == "Save") {
                const xhttp = new XMLHttpRequest();
                xhttp.onload = function () {
                    console.log('success');

                    $.get(`/comment/journey/${e.target.id}`, function (data) {
                        $(`#${e.target.id}-comments`).html(data);
                        submitBtn.val("Post");
                        commentId.val(0);
                        createAlert('', '', 'Comment was updated Successfully', 'success', true, true, 'pageMessages');
                    });
                }
                xhttp.open("POST", `/comment/UpdateComment/`);
                let data = new FormData(e.target)
                xhttp.send(data);
            }
            else {
                commentId.val(0);
                const xhttp = new XMLHttpRequest();
                xhttp.onload = function () {
                    console.log('success');

                    $.get(`/comment/journey/${e.target.id}`, function (data) {
                        $(`#${e.target.id}-comments`).html(data);
                    });
                    createAlert('', '', 'Comment was Posted Successfully', 'success', true, true, 'pageMessages');
                }
                xhttp.open("POST", `/comment/addcomment/`);
                let data = new FormData(e.target)
                xhttp.send(data);
            }

            $("textarea[name=Body]", e.target).html("");
            $("textarea[name=Body]", e.target).val("");
        }


        function DeleteComment(id, journeyId) {
            $.get(`/comment/DeleteComment/${id}`, function (data) {
                $.get(`/comment/journey/${journeyId}`, function (data) {
                    $(`#${journeyId}-comments`).html(data);
                    createAlert('', '', 'Comment was deleted Successfully', 'success', true, true, 'pageMessages');
                });
            });
        }


        function EditComment(id, journey_Id) {
            console.log(id, journey_Id);
            console.log($(`#comment-${id}-body`).html());
            $(`#Body-${journey_Id}`).html($(`#comment-${id}-body`).html());
            $(`#Body-${journey_Id}`).val($(`#comment-${id}-body`).html());
            $(`#submitBtn-${journey_Id}`).val("Save");
            $(`#${journey_Id}-Comment-Id`).val(id);
        }


        var btn = $('#baccToTopBtn');

        $(window).scroll(function () {
            if ($(window).scrollTop() > 300) {
                btn.addClass('show');
            } else {
                btn.removeClass('show');
            }
        });

        btn.on('click', function (e) {
            e.preventDefault();
            $('html, body').animate({ scrollTop: 0 }, '300');
        });

        function CancelRequest(id) {
            $.get(`/Request/Cancel/${id}`, function (data) {
                console.log(data);
            });
        }

        var slider = document.getElementById("myRange");


        // Update the current slider value (each time you drag the slider handle)
        slider.oninput = function () {
            slider.value = this.value;

        }

        function RateJourney(btn) {
            var Journyid = $(btn).attr('data-JournyId');
            var userId = $(btn).attr('data-userId');

            $("#UserId").val(userId);
            $("#JournyId").val(Journyid);
            // As pointed out in comments,
            // it is unnecessary to have to manually call the modal.
            // $('#addBookDialog').modal('show');
        }
    </script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
